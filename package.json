{
  "name": "prettier-standard-formatter",
  "version": "0.1.0",
  "description": "Prettier for JavaScript Standard Style",
  "main": "src/index.js",
  "bin": "src/cli.js",
  "license": "MIT",
  "dependencies": {
    "bluebird": "^3.5.0",
    "get-stdin": "^5.0.1",
    "globby": "^6.1.0",
    "meow": "^3.7.0",
    "mz": "^2.6.0",
    "prettier": "^0.22.0",
    "ramda": "^0.23.0",
    "recursive-readdir": "^2.1.1",
    "standard": "^9.0.2"
  },
  "devDependencies": {
    "faucet": "0.0.1",
    "mkdirp": "^0.5.1",
    "rimraf": "^2.6.1",
    "semistandard": "^10.0.0",
    "snazzy": "^6.0.0",
    "tap-xunit": "^1.7.0",
    "tape": "^4.6.3"
  },
  "scripts": {
    "format": "node ./src/cli.js ./**/*.js",
